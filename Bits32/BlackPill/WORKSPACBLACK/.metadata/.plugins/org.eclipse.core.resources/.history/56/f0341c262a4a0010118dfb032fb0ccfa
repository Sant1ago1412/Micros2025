#ifndef INC_ADC_H_
#define INC_ADC_H_

#include <stdint.h>

#define NUMCHANNELS 8

typedef struct{
	uint16_t raw_Data[8];
	uint16_t filteredData[8];
	uint32_t Sumatoria[8];
	uint8_t newValue;
}s_ADC;

void ADC_Task();
void ADC_Filter();
void ADC_Init(s_ADC *AdcHandler);
#endif /* INC_ADC_H_ */
